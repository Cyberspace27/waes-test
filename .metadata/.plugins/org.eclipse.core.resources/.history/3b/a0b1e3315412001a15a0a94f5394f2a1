	  waitTime(3000);	
  }

  @AfterClass
  public void afterClass() {
  }
  
  
  @Test
  public void fillSingUpInvalid() {
	  signUpPage.enterSignUpData("cyber","naruto","ariel",11,10,1988);
	  
	  
  }
  

